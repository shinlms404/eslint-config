{
  "name": "@shinlms404/eslint-config",
  "version": "1.0.1",
  "description": "My ESLint config preset",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/shinlms404/eslint-config#readme",
  "bugs": "https://github.com/shinlms404/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinlms404/eslint-config.git"
  },
  "license": "MIT",
  "author": "Charlie Chan <shinlms404@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "npm run build",
    "prepare": "npx simple-git-hooks",
    "release": "bumpp && npm publish",
    "stub": "tsup --format esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --format esm,cjs --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.1",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@shinlms404/prettier-config": "^0.1.6",
    "@types/node": "^20.17.23",
    "bumpp": "^9.11.1",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "simple-git-hooks": "^2.11.1",
    "sucrase": "^3.35.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "eslint": "^8.57.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
